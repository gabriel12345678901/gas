function criarEmail() {
 
 // variavel ss recebe todas as propriedades da planilha ativa:
 var ss = SpreadsheetApp.getActiveSpreadsheet();
 //acessa a aba ações
 ss.setActiveSheet(ss.getSheetByName("email"));
 //variavel sheet recebe as propriedades da aba, é a única maneira de acessar as células:
 var sheet = SpreadsheetApp.getActiveSheet();
 //variavel fim busca a primeira linha não preenchida da coluna CJ
 var fim = sheet.getRange('C1').getNextDataCell(SpreadsheetApp.Direction.DOWN).getRow();  
 //capta ultima linha preenchida:
 var linfim = fim-1
  
 //contador i, inicio do loop na linha 2
 var i = 2
 
 //loop for, laço repetidor que contara da linha 2 até a ultima linha preenchida:
 for(i;i < linfim; ++i){
  
     
     var mensagem = sheet.getRange(i, 1).getValue();
     var emailAddress = sheet.getRange(i, 2).getValue();
     var assunto = "Documentos não entregues"
     
     if(emailAddress != ""){
      MailApp.sendEmail(emailAddress,assunto,mensagem); 
      //Browser.msgBox('entrou no if email: '+emailAddress)
     } 
  
 
  }//fecha o for
  
}//fecha a function
